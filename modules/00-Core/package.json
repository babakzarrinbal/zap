{
  "name": "@zzapp/core",
  "version": "1.0.2",
  "description": "core component",
  "main": "dist/index.js",
  "browser": "dist/browser.js",
  "scripts": {
    "prebuild":"npx cpy **/*.sql ../dist/src-electron/ --cwd=../../src-electron/ --no-overwrite --parents ",
    "start": "npx ts-node -r tsconfig-paths/register ",
    "build2": "npx del-cli dist &&npx tsc &&npx tscpaths -p tsconfig.json -s ./src -o ./dist && npx cpy **/*.sql ../dist/ --cwd=src/ --no-overwrite --parents",
    "build": "npx del-cli dist &&npx tsc &&npx tscpaths -p tsconfig.json -s ./src -o ./dist ",
    "build-rollup": "npx del-cli dist &&npx rollup -c",
    "lint": "eslint --fix 'src/**/*.ts'",
    "test": "jest"
  },
  "author": "babak.zarrinbal",
  "email": "babak.zarrinbal@gmail.com",
  "license": "ISC",
  "dependencies": {
    "handlebars": "^4.7.7",
    "lodash": "^4.17.21",
    "pino": "^6.11.3",
    "sql.js": "^1.6.2",
    "sqlite3": "^5.0.2",
    "zap": "file:../.."
  },
  "engines": {
    "node": ">= 12.*",
    "npm": ">= 6.*"
  },
  "devDependencies": {
    "@open-wc/building-rollup": "^2.0.1",
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/jest": "^27.4.0",
    "cpy-cli": "^3.1.1",
    "deepmerge": "^4.2.2",
    "del-cli": "^4.0.1",
    "jest": "^27.4.7",
    "rollup": "^2.66.1",
    "ts-jest": "^27.1.3",
    "tsconfig-paths": "^3.12.0",
    "tscpaths": "^0.0.9",
    "typescript": "^4.5.5"
  }
}
